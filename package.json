{
  "name": "prometheus",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "npm run build -ws",
    "lint": "npm run lint --workspaces --if-present",
    "dev": "npm run start:dev -w @prometheus/api & npm run dev -w @prometheus/web",
    "prepare": "npm run build -w @prometheus/types",
    "typecheck": "tsc --noEmit -ws"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
